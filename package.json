{
  "name": "metafar-fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prepare": "husky",
    "build": "tsc && vite build",
    "typecheck": "tsc --noEmit",
    "analyze": "mkdir -p docs/metrics/bundle && vite-bundle-visualizer --output docs/metrics/bundle/stats.html --open true",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix --report-unused-disable-directives",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "*.{json,md,css,scss,html}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.2.9",
    "@mui/material": "^5.15.17",
    "@mui/system": "^5.15.15",
    "@tanstack/query-async-storage-persister": "5.90.14",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/react-query-persist-client": "^5.90.14",
    "@tanstack/react-virtual": "^3.13.13",
    "axios": "^1.6.8",
    "highcharts": "^11.4.1",
    "highcharts-react-official": "^3.2.1",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^6.23.1",
    "react-spinners": "^0.13.8",
    "zod": "^4.2.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "4.0.16",
    "@vitest/ui": "4.0.16",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.7.4",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-bundle-visualizer": "^1.2.1",
    "vitest": "4.0.16"
  }
}
